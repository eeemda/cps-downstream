@unpublished{Rogger2013,
  title = {Management of {{Bureaucrats}} and {{Public Service Delivery}}: {{Evidence}} from the {{Nigerian Civil Service}}},
  author = {Rogger, Daniel and Rasul, Imran},
  year = {2013},
  abstract = {We study how the management practices under which public sector bureaucrats operate, correlate to the quantity and quality of public services delivered. We do so in the context of a developing country, exploiting novel data from the Nigerian Civil Service linking public sector organizations to the individual projects they are responsible for. For each of 4700 projects, we have hand coded detailed independent engineering assessments of each project's comple- tion rate and delivered quality. We supplement this information with a survey we conducted to elicit management practices for bureaucrats in each of the 63 civil service organizations responsible for these projects, following the approach of Bloom and Van Reenen [2007]. We ldotsnd thatmanagement practices related to autonomy signildotscantly improve project completion rates and project quality. However, we ldotsnd management practices related to performance- based incentives signildotscantly reduce project completion rates and project quality. We explore how the impacts of both dimensions of management practice vary by project characteristics, such as their scale and complexity, by organizational characteristics, such as sta\textcurrency{} qualildotsca- tions andmanagerial spans of control, and by bureaucrat characteristics, such as their tenure, intrinsic motivation and perceptions of corruption in the organization. Our ldotsndings provide among the ldotsrst evidence to quantify the potential gains to public service delivery arising from changes in how civil service bureaucrats are managed.},
  mendeley-tags = {Autonomy,Bureaucracy,Bureaucratic Quality,Development,Public Goods},
  keywords = {Africa,Autonomy,Bureaucracy,Bureaucratic Quality,Development,Nigeria,performance evaluation,Performance Pay,Public Goods,public service delivery,State Autonomy}
}

