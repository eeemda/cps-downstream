  keywords = {DISE,Education,India,Institutions,p16,Political Business cycle,Political Competition,political cycle,Primary Schools,Public Goods,school resources,study and helpful discussions,thank nuepa and the,the key datasets used,this paper has benefited,voter turnout}
}

