@article{Kapur2007,
  title = {Sisyphean {{State}}? {{Why}} Poverty Programs in {{India}} Fail and yet Persist},
  author = {Kapur, Devesh and Mukhopadhyay, Partha},
  year = {2007},
  number = {August},
  abstract = {Independent India's commitment to democratic politics meant that its polity had to grapple with the harsh reality of India's poverty: the sheer number of the poor (who were also now voters), the intensity of poverty and destitution and a deeply stratified and hierarchical society. Addressing the needs of vulnerable and marginalized groups in society has preoccupied the energies of intellectuals and policy makers and a focus of political rhetoric in India to a degree that is uncommon among developing countries. India has deployed a vast array of programs with different instruments and objectives. Yet, notwithstanding their decidedly modest record, India has persisted with ever new cycles of poverty programs. What explains the continual failure and why does India persist with these programs? This paper attempts to address this puzzle by first examining the range of poverty programs that the Indian state has employed to improve the welfare of its many marginalized and vulnerable citizens. We first provide a typology of the more important of these interventions and then examine both the rationale underpinning these interventions and their outcomes based primarily on the evaluations by Indian state agencies. We then focus on the principal reasons for the failure of these programs locating them in key characteristics of India's political economy. We argue that all poverty programs must necessarily pass through the eve of the needle \textendash{} local public administration, where all anti-poverty programs are implemented. The chronic weakness of local public administration pretty much ensures that successful poverty programs will only be an exception and poor performance the rule. We further argue that the weakness of local public administration is partially endogenous to the structure of electoral incentives, which has contributed to an electoral equilibrium. While the political imperatives driving targeted poverty programs despite chronic failure are not surprising, what is more puzzling \textendash{} and needs explanation \textendash{} is less why these countless targeted interventions have performed poorly, but rather why the state, international agencies and India's intellectual class persist in reposing faith in these instruments. Finally the paper examines alternatives and why they have not been tried. ii},
  mendeley-tags = {Central Sector Schemes,Development,India,Public Services},
  keywords = {Central Sector Schemes,Development,India,public services}
}

