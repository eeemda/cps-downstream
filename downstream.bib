@phdthesis{Chauchard2011,
  title = {From {{Political Power}} to {{Changing Group Relations}}? {{Tracking}} the {{Psychological Impact}} of {{Political Inclusion}} in {{Rural India}}},
  author = {Chauchard, Simon},
  year = {2011},
  abstract = {Can the access to political power by a few members of a socially disadvantaged group trickle down to city streets and change common beliefs related to members of that group? Can it, in turn, alter the nature of day-to-day interpersonal relations between members of the different groups? To answer these questions, this dissertation focuses on recent efforts to include members of the scheduled castes (the ex-``untouchables'') through the use of "reservation" for key executive positions in India's village institutions. These radical quotas, in place since the mid-1990's, abruptly allowed members of the scheduled castes to be elected as heads of village councils, in a bold move disregarding the on-going social practice of untouchability. Drawing on ethnographic fieldwork in the Northern Indian state of Rajasthan, I suggest that this political inclusion may affect a wide range of beliefs related to members of the scheduled castes (beliefs about their qualities, their place in society, social norms in interacting with them, or the consequence of hostile behavior towards them, etc\textbackslash ldots). To test these intuitions, the dissertation combines a ``natural experiment'' with two Large-N audio surveys ran across 64 villages, and compares the untouchability-related attitudes of villagers who have experienced political power by a member of the scheduled castes to the attitudes of villagers who have not experienced it. Results suggest that the experience of a member of the scheduled castes in office significantly increased inter- castes contact and affected a number of beliefs among members of all groups. In particular, members of the scheduled castes that experienced a co-ethnic in office reported perceiving less discrimination, while members of dominant groups developed new norms of tolerance and were more likely to believe that hostile behaviors would not go unpunished. More importantly, as indicated by further analyses, this experience may have developed more assertive behaviors among members of the scheduled castes, and reduced hostile and segregating behaviors among dominant castes. Altogether, these findings suggest that the experience of political leadership by a member from a disadvantaged group can significantly improve the daily wellbeing of other individuals from those groups.},
  mendeley-tags = {Ethnic Conflict,Ethnicity,Field Experiment,India},
  school = {New York University},
  keywords = {Ethnic Conflict,Ethnicity,Field Experiment,India}
}

