@unpublished{Chakrabarti2009a,
  title = {What Is Driving the {{Indian}} Middle Class towards Electoral Politics? {{Evidence}} from {{Delhi}}},
  author = {Chakrabarti, Poulomi},
  year = {2009},
  address = {{New Delhi}},
  abstract = {Neighborhood Associations have assumed an important role in public policy decision making as the principal voice of the middle class across urban India. In recent years, these associations have sought a more formal role in policy making by contesting for political office in local elections in at least five major cities. This is significant not only because of its ``newness'' and implications on public policy but also because it represents a blurring of boundaries between the civil society, traditionally characterized by its `apolitical' nature, and the political society which has largely been the domain of the poor. Using the case of Delhi and drawing on examples from other metropolitan cities, this paper attempts to understand the factors that have led to the rise of middle class neighborhood associations and their subsequent advent into formal politics. The unique institutional environment of the capital and a Government of Delhi program that institutionalized citizen participation through these very associations makes Delhi a particularly interesting case. I discover that neighborhood associations with pre-existing networks with government agencies were more likely to remain apolitical, while the ones without these networks seek to become part of the government machinery through formal electoral channels. Policies on decentralization instituted by the Central and the State Government have been instrumental in this change. 4},
  mendeley-tags = {Delhi,Democratization,India,Local Democracy,Local Government,Municipal Government,Neighbourhood Associations,Social Capital,Urban Politics,Urbanization},
  keywords = {Delhi,Democratization,India,Local Democracy,Local Government,Municipal Government,Neighbourhood Associations,Social Capital,Urban Politics,Urbanization}
}

@article{Chakrabarti2013,
  title = {Disaggregating {{Social Development}}: {{The Variant Politics}} of {{Education}} and {{Healthcare}} in {{India}}},
  shorttitle = {Disaggregating {{Social Development}}},
  author = {Chakrabarti, Poulomi and Mistree, Dinsha},
  year = {2013},
  abstract = {Comparative studies of social development are generally developed and tested by comparing territories. In this process, different forms of public goods provision are often combined to understand why a particular state or country fares better than others. But is something lost by assuming that the politics driving the various forms of social development fit the same rubric within a given territory? In this paper, we explore the variation in government support for two of the most recognized components of social development: education and healthcare. Using a novel dataset of Indian state-level public expenditure from 1958 to 2008, we find that sector classifications capture a considerable amount of the variation in overall patterns of public spending. While public spending for education and healthcare remain low for much of India's history, public spending for education shows a marked increase since the early 1990s, while healthcare continues to grow at a slow pace. A close examination of education and healthcare policy in the last six decades suggests that the prioritization of mass education may be explained by its perceived association to economic growth while the connection between healthcare and growth is still at a nascent stage in policy discourse. Our research strongly suggests that scholars need to go beyond the cross-territorial model if we are to truly understand the larger processes that guide social development.},
  keywords = {Development,Education,Healthcare,India,Social Spending,Welfare State}
}

