@phdthesis{Harding2012a,
  title = {Democracy and {{Rural Bias}} in {{Sub-Saharan Africa}}},
  author = {Harding, Robin},
  year = {2012},
  abstract = {The question motivating my dissertation is: why are urbanites in Africa hostile to- wards incumbent governments? Although there is widespread acceptance that African electoral politics is dominated by ethnicity and vote-buying, not by an urban/rural cleavage, urban residents across Africa are significantly less likely to support incum- bents. Inanswering this question, I argue that democracy makes African governments more responsive to rural interests; because a majority of Africans live in rural areas, competitive elections create incentives for governments to implement policies that favor rural interests while ignoring the urban minority, thereby generating dissatis- faction among urban voters. The logical implication of this argument is that this hos- tility should reduce as urbanization increases, because governments gain incentives to more evenly balance pro-rural and pro-urban policies. Importantly, this argument assumes that voters in Africa are responsive to public goods. I investigate the validity of this theoretical argument in three ways. In the first pa- per, I use cross-national survey data to show that while urban residents are less likely to support incumbents, this difference is conditional on the level of urbanization. In the second paper I investigate whether democracy leads to a ``rural bias'' in public goods, using individual level data on education attainment across 29 countries. The results provide strong evidence of a rural bias in the impact of democracy on educa- tion attainment across Africa. Moreover, they show that this rural bias is conditional on the level of urbanization. In the third paper I investigate the argument's foun- dational assumption \textendash{} that citizens in Africa vote on the basis of public goods \textendash{} by analyzing the relationship between presidential vote shares and road conditions in Ghana. Using an original dataset, I show that vote shares are significantly and sub- stantially affected by changes to local road conditions. My findings show that, as the theory predicts, democracy has a positive impact on the provision of rural public goods. In addition, they demonstrate that African politics is not only about ethnicity and vote-buying; voters in Africa care about public goods, and therefore elections create incentives for incumbents to target resources to the rural majority, which generates hostility fromurban residents.},
  mendeley-tags = {Africa,Anti-Incumbency,Democracy,Education,Incumbency,Public Goods,Public Goods Provision,Urban-Rural Cleavage,Urbanization},
  keywords = {Africa,Anti-Incumbency,Democracy,Education,incumbency,Public Goods,public goods provision,Urban-Rural Cleavage,Urbanization}
}

@article{Harding2014,
  title = {What {{Democracy Does}} (and {{Doesn}}'t Do) for {{Basic Services}}: {{School Fees}}, {{School Inputs}}, and {{African Elections}}},
  author = {Harding, Robin and Stasavage, David},
  year = {2014},
  journal = {The Journal of Politics},
  volume = {76},
  number = {1},
  pages = {229--245},
  abstract = {Does democracy affect the provision of basic services? We advance on existing empirical work on this subject by exploring the potential mechanisms through which a democratic transition may prompt a government to alter provision of basic services to its citizens. In an environment of weak state capacity, in which it is di\textcent{} cult for voters to attribute outcomes to executive actions, we suggest that electoral competition is most likely to lead to changes in policies where executive action is verildotsable. Considering the context of African primary education as an example, we suggest that electoral competition will therefore give governments an incentive to abolish school fees, but it will have less e\textcurrency ect on the provision of school inputs, precisely because executive actions on these issues are more di\textcent{} cult to monitor. We evaluate this claim by approaching it in three di\textcurrency erent ways, using cross-national as well as individual level data, including an original data set on primary school fee abolitions. First we show that in Africa, democracies have higher rates of school attendance than non-democracies. Moreover, evidence suggests that this is primarily due to the fact that democracies are more likely to abolish school fees, not to the fact that they provide more inputs. We then estimate the likelihood that a government will abolish school fees subsequent to an election, taking account of endogeneity concerns involving election timing. Finally, we use survey data from Kenya to provide evidence suggesting that citizens condition their voting intentions on an outcome that a politician can control directly, in this case abolishing school fees, but not on outcomes over which politicians have much more indirect in\textdaggerdbl uence, such as local school quality.},
  mendeley-tags = {Africa,Democracy,Development,Education,Kenya,User Fees},
  keywords = {Africa,Democracy,Democratization,Development,Education,Kenya,Primary Education,User Fees}
}

@article{Harma2009,
