@unpublished{Milner2012a,
  title = {Which {{Devil}} in {{Development}}? {{A Large-N Survey}} and {{Randomized Field Experiment Comparing Bilateral Aid}}, {{Multilateral Assistance}}, and {{Government Action}} in {{Uganda}}},
  author = {Milner, Helen V. and Nielson, Daniel L. and Findley, Michael G.},
  year = {2012},
  abstract = {The debate over the effectiveness of foreign aid has raged on despite a paucity of information about recipients' actual views of aid. We address this deficiency with results from a national survey and field experiment on roughly 3,600 Ugandan citizens that includes several behavioral outcomes. We argue that citizens may support foreign aid because it offers a way out of the voter's dilemma (Lyne 2007, 2008) where clientelistic networks compel voters to take particularistic payoffs in lieu of public goods. Citizens may perceive foreign aid as relatively less politicized and less prone to corruption compared to government programs. They may also favor multilateral donors over bilateral donors for the same reason: they may perceive multilateral aid as less politicized and more transparent than bilateral aid. In the survey and experiment, we find that foreign aid is viewed very positively among recipients and that Ugandans favor aid donors consistently over the (quite popular) national government. In a randomized field experiment, we also find that citizens are significantly more willing to tell their local representatives that they support randomly assigned foreign aid projects, sign a petition, or send a text message in favor of the aid relative to government programs. Subjects familiar with multilateral donors were significantly more likely to sign a petition indicating their support compared to bilateral aid and government programs. Thus, our evidence indicates that citizens perceive aid generally, and multilateral aid in particular, as more transparent and less prone to politicization. The findings suggest that recipients view foreign aid \textendash{} particularly from multilateral donors \textendash{} as relatively effective and are willing to undertake costly action to support it, especially compared to the most likely alternative of government programs.},
  keywords = {Development,Foreign Aid,Randomized Evaluation}
}

