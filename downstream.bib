@unpublished{Staniland2013,
  title = {Militarized {{Elections}} and the {{Politics}} of {{Violence}} in {{South Asia}}},
  author = {Staniland, Paul},
  year = {2013},
  abstract = {Governments and ruling parties often unleash non-state armed groups as an electoral strategy. Yet while much attention has been focused on the origins of electoral violence, we know little about the long-run consequences of this form of ``outsourced'' violence. The puzzle driving this paper is why, following initial electoral militarization, armed groups are co-opted into ruling parties, violently turn on their former government allies, persist as enduringly armed parts of the political system, or are quickly suppressed when no longer useful. I analytically identify these trajectories as incorporation, blowback, normalization, and purging. The paper argues that post-militarization trajectories are driven by the intersection of government political interests and armed group organizational autonomy. Comparative evidence from South Asia shows the importance of these concepts and processes, which are relevant to a much broader range of cases. The specter of violence needs be firmly integrated into the study of elections, state building, and political order.},
  mendeley-tags = {Cooptation,Elections,Non-State Actors,South Asia,Violence},
  keywords = {Cooptation,Elections,Non-State Actors,South Asia,Violence}
}

@article{Stasavage2005,
