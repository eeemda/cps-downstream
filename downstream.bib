@unpublished{Andrabi2010,
  title = {Bad {{Public Schools}} Are {{Public Bads}}: {{Civic Values}} and {{Test-Scores}} in {{Public}} and {{Private Schools}}},
  author = {Andrabi, Tahir and Bau, Natalie and Das, Jishnu and Khwaja, Asim Ijaz},
  year = {2010},
  abstract = {During the last decade, enrollments in private schools exploded in a number of low-income countries. In both India and Pakistan, for example, private schools, the majority of which operate as small-scale, low cost enterprises without any regulatory oversight or government subsidies, now account for more than 20 percent of all primary enrollments. The environment that these schools operate in and their increasing importance offer a unique opportunity to evaluate the benefits (or lack thereof) of a pure market model of educational provision with no government regulation or subsidies. Using an instrumental variables approach together with data from Pakistan, we show that test-scores of children in private schools are 0.8 to 1 standard deviation higher (depending on the subject) than those of their public school counterparts. Further, public schools are not better at promoting better civic values; if anything, children in private schools are better informed about Pakistan, are more "pro-democratic," and exhibit lower gender biases. These results question the oft-posited trade-off between private schooling and ideology. Given that the cost of educating a child in a public school is 2 to 5 times as much as in a private school, they also argue for rethinking the fundamental model of how to deliver education in low-income settings.},
  mendeley-tags = {Citizenship,Education,Education Markets,Pakistan,Private Schools},
  keywords = {Citizenship,Education,Education Markets,Pakistan,Private Schools}
}

